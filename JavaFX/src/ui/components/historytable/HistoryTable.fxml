<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.components.historytable.HistoryTableController">
   <children>
      <TableView fx:id="table">
        <columns>
          <TableColumn fx:id="runNoColumn" prefWidth="75.0" text="Run Num" />
          <TableColumn fx:id="degreeColumn" prefWidth="75.0" text="Degree" />
            <TableColumn fx:id="outputColumn" prefWidth="75.0" text="Output" />
            <TableColumn fx:id="cyclesColumn" prefWidth="75.0" text="Cycles" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <AnchorPane pickOnBounds="false" StackPane.alignment="BOTTOM_CENTER">
         <children>
            <HBox fx:id="historyBtns" alignment="BOTTOM_CENTER" fillHeight="false" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <children>
                  <Button fx:id="rerunButton" mnemonicParsing="false" onAction="#onRerunClicked" text="Rerun" />
                  <Button mnemonicParsing="false" text="Show" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
